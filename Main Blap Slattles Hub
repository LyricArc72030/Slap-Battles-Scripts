if Link.Success == true then

   loadstring(Link.Body)()    

else    

    if Link.Success == false and game.PlaceId == 11520107397 or game.PlaceId == 9015014224 then

        loadstring(game:HttpGet("https://raw.githubusercontent.com/LyricArc72030/Slap-Battles-Scripts/main/Blap%20Slattles%20Hub"))()        

    else        

        error("GAME IS NOT SUPPORTED!")

    end    

end
